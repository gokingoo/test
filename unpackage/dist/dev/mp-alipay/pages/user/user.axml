<view><view class="border-black flex-row align header"><block a:if="{{!userData.user_id}}"><image class="header-photo" resize="cover" src="/static/images/head_unlogin@2x.png"></image></block><block a:if="{{userData.user_id}}"><image class="header-photo" resize="cover" src="{{HeadLoginImage}}"></image></block><view style="margin-left:30rpx;"><block a:if="{{userData.user_id}}"><view class="flex-row align"><text class="mobile-txt">{{userData.user_name}}</text><image class="vip-img" src="{{vipImg}}"></image><text class="vip-text" style="{{(vipColor)}}">{{userData.level_name}}</text></view></block><block a:if="{{userData.user_id}}"><view class="flex-row" style="margin-top:10rpx;"><block a:if="{{userData.user_state==0}}"><text data-event-opts="{{[['tap',[['toAuth',['$event']]]]]}}" class="btn-non-authen" onTap="__e">未认证</text></block><block a:if="{{userData.user_state==1||userData.user_state==2}}"><text data-event-opts="{{[['tap',[['toAuth',['$event']]]]]}}" class="btn-authen" onTap="__e">已认证</text></block><block a:if="{{userData.user_state==3}}"><text class="btn-non-authen">冻结</text></block><text data-event-opts="{{[['tap',[['toIncome',['$event']]]]]}}" class="btn-authen" style="margin-left:15rpx;" onTap="__e">{{"现金账户 ￥"+userData.cash}}</text></view></block></view></view><view class="border-contain flex-row  _div" style="padding:50rpx;margin-left:15rpx;margin-right:15rpx;"><view data-event-opts="{{[['tap',[['toShare',['$event']]]]]}}" class="flex1 align _div" style="display:flex;flex-direction:column;" onTap="__e"><image class="share-img" src="/static/images/share_img@2x.png"></image><text class="text-black">分享有礼</text><text class="text-yellow">赠200元券</text></view><view style="width:1px;height:150rpx;background-color:#cccccc;" class="_div"></view><view data-event-opts="{{[['tap',[['toWallet',['$event']]]]]}}" class="flex1 align _div" style="display:flex;flex-direction:column;" onTap="__e"><image class="share-img" src="/static/images/wallet_img@2x.png"></image><text class="text-black">我的钱包</text><text class="text-yellow">储值享折扣</text></view></view><view class="border-contain" style="margin:15rpx;"><view data-event-opts="{{[['tap',[['toOrder',['$event']]]]]}}" class="flex-row align" style="padding:30rpx;" onTap="__e"><image class="share-img" src="/static/images/order_img@2x.png"></image><text class="flex1 text-black" style="margin-left:10px;">我的订单</text><image class="point-right" src="/static/images/point_right@2x.png"></image></view><view style="width:650px;height:1px;background-color:#cccccc;margin-left:0rpx;"></view><view data-event-opts="{{[['tap',[['toInvoice',['$event']]]]]}}" class="flex-row align" style="padding:30rpx;" onTap="__e"><image class="share-img" src="/static/images/invoice_img@2x.png"></image><text class="flex1 text-black" style="margin-left:10px;">申请发票</text><image class="point-right" src="/static/images/point_right@2x.png"></image></view></view><view data-event-opts="{{[['tap',[['toContact',['$event']]]]]}}" class="border-contain flex-row align" style="padding:30rpx;margin-left:6rpx;margin-right:15rpx;" onTap="__e"><image class="phone-img" src="/static/images/phone_img@2x.png"></image><text class="flex1 text-black" style="margin-left:10px;">联系我们</text><image class="point-right" src="/static/images/point_right@2x.png"></image></view><view data-event-opts="{{[['tap',[['toSetting',['$event']]]]]}}" class="border-contain flex-row align" style="padding:30rpx;margin:15rpx;" onTap="__e"><image class="share-img" src="/static/images/setting_img@2x.png"></image><text class="flex1 text-black" style="margin-left:10px;">设置</text><image class="point-right" src="/static/images/point_right@2x.png"></image></view><block a:if="{{user_id}}"><view data-event-opts="{{[['tap',[['quit',['$event']]]]]}}" class="border-contain flex-row align" style="padding:30rpx;margin:15rpx;" onTap="__e"><image class="share-img" src="/static/images/quit@2x.png"></image><text class="flex1 text-black" style="margin-left:10px;">退出</text><image class="point-right" src="/static/images/point_right@2x.png"></image></view></block></view>