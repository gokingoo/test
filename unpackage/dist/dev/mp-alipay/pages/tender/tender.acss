.img1 {
  width: 720rpx;
  height: 400rpx;
}
.img2 {
  width: 720rpx;
  height: 732rpx;
}
.contain {
  background-color: #ffffff;
  border-radius: 15px;
}
.confirm-btn {
  height: 80rpx;
  text-align: center;
  background-color: #333333;
  font-size: 25rpx;
  line-height: 40px;
  border-width: 1px;
  border-color: #989898;
  border-radius: 10px;
  color: #ffffff;
}
.btn-income {
  flex-direction: row;
  align-items: center;
  justify-content: center;
  width: 670px;
  height: 80px;
  margin-top: 20px;
  margin-bottom: 20px;
  background-color: #ffffff;
  border-radius: 15px;
  border-width: 1px;
  border-color: #999999;
}
.income-img {
  width: 35px;
  height: 35px;
}
.car-tender {
  width: 45px;
  height: 35px;
  margin-right: 10px;
